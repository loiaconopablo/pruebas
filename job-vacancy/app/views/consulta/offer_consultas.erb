<h1>Consultas de la oferta <%=@job.title %> </h1>
<table class="table table-bordered">
	<tr>
		<th>Titulo</th>
		<th>Description</th>
		<th>Autor de la consulta</th>
		<th>Acciones</th>
	</tr>
	<% @consultas_find.each do | consu| %>

	<tr>
			<td>
			<%= consu.title %>
		</td>
		<td>
			<%= consu.description %>
		</td>
		<td>
			<%= consu.user.email %>
		</td>		
		<td>
			<%- form_tag url(:consulta, :destroy, :consu_id => consu.id), :method => :delete, :style => 'display:inline' do %>
                <button type='submit' title="Delete offer" rel='tooltip' class="btn btn-secondary" >Delete
                </button>
       <% end %>
		</td>	
	</tr>
<% end %>